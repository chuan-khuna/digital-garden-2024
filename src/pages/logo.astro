<script>
  // Get the exact path length for precise animation
  const path = document.querySelector('.animated-path') as SVGPathElement;
  const tBar = document.querySelector('.t-bar') as SVGPathElement;
  
  if (path) {
    const length = path.getTotalLength();
    // Set the dash array and offset to the actual path length
    path.style.strokeDasharray = `${length}`;
    path.style.strokeDashoffset = `${length}`;
  }
  
  if (tBar) {
    const tBarLength = tBar.getTotalLength();
    // Set the dash array and offset for the t-bar
    tBar.style.strokeDasharray = `${tBarLength}`;
    tBar.style.strokeDashoffset = `${tBarLength}`;
  }
</script>
