<style>
  .logo-container {
    display: flex;
    justify-content: center;
    align-items: center;
    min-height: 100vh;
    background: white;
  }

  .masked-logo {
    max-width: 100%;
    height: auto;
  }

  .animated-path {
    stroke-dasharray: 5000;
    stroke-dashoffset: 5000;
    animation: draw 2.5s ease-in-out forwards;
  }

  .t-bar {
    stroke-dasharray: 200;
    stroke-dashoffset: 200;
    animation: draw-tbar 0.5s ease-in-out forwards;
    animation-delay: 2.5s;
  }

  @keyframes draw {
    to {
      stroke-dashoffset: 0;
    }
  }

  @keyframes draw-tbar {
    to {
      stroke-dashoffset: 0;
    }
  }
</style>

