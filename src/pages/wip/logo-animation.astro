<script>
  // Get precise path lengths for accurate animation
  const leftStroke = document.querySelector('.stroke-left') as SVGPathElement
  const middleStroke = document.querySelector(
    '.stroke-middle',
  ) as SVGPathElement
  const rightStroke = document.querySelector('.stroke-right') as SVGPathElement

  if (leftStroke) {
    const length = leftStroke.getTotalLength()
    leftStroke.style.strokeDasharray = `${length}`
    leftStroke.style.strokeDashoffset = `${length}`
  }

  if (middleStroke) {
    const length = middleStroke.getTotalLength()
    middleStroke.style.strokeDasharray = `${length}`
    middleStroke.style.strokeDashoffset = `${length}`
  }

  if (rightStroke) {
    const length = rightStroke.getTotalLength()
    rightStroke.style.strokeDasharray = `${length}`
    rightStroke.style.strokeDashoffset = `${length}`
  }
</script>
