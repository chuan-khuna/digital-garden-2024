<style>
  .logo-container {
    display: flex;
    justify-content: center;
    align-items: center;
    min-height: 100vh;
    background: white;
  }

  .masked-logo {
    max-width: 400px;
    height: auto;
  }

  /* Animate the three vertical strokes from left to right, top to bottom */
  /* Each stroke waits for the previous one to finish */
  .stroke-left {
    stroke-dasharray: 1000;
    stroke-dashoffset: 1000;
    animation: draw-stroke 1s ease-in-out forwards;
    /*erase-stroke 0.6s ease-in-out forwards;*/
    animation-delay: 0s, 5.2s;
  }

  .stroke-middle {
    stroke-dasharray: 1000;
    stroke-dashoffset: 1000;
    animation: draw-stroke 1s ease-in-out forwards;
    /*erase-stroke 0.6s ease-in-out forwards;*/
    animation-delay: 1s, 4.6s;
  }

  .stroke-right {
    stroke-dasharray: 1000;
    stroke-dashoffset: 1000;
    animation: draw-stroke 1s ease-in-out forwards;
    /*erase-stroke 0.6s ease-in-out forwards;*/
    animation-delay: 2s, 4s;
  }

  @keyframes draw-stroke {
    to {
      stroke-dashoffset: 0;
    }
  }

  @keyframes erase-stroke {
    from {
      stroke-dashoffset: 0;
    }
    to {
      stroke-dashoffset: 1000;
    }
  }
</style>

